<?xml version="1.0" encoding="UTF-8"?>
<project default="jar">

	<target name="jar">
		<mkdir dir="jar" />
		<jar destfile="jar/webcachedigger1.0.jar" basedir="bin">
			<manifest>
				<attribute name="Main-Class" value="org.sergeys.webcachedigger.WebCacheDigger" />
			</manifest>
		</jar>
		<echo>packed</echo>
	</target>

	<target name="clean">
		<delete dir="jar" />
	</target>
	
</project>
